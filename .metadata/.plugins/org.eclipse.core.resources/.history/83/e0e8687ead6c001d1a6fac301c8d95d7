package utils;

import org.openqa.selenium.WebDriver;

public class FecharDriver {

	public WebDriver driver;

	//criação de construtor do driver
	//para entregar um objeto de um modo específico
	public FecharDriver(WebDriver driver) {
		//this. para diferenciar o parâmetro do atributo
		this.driver = driver;
	}
	
	//função para fechar o driver
	public void fechar_driver() {
		driver.quit();
	}
}
