package utils;

import org.openqa.selenium.WebDriver;

import io.cucumber.java.After;
import pages.BuscaBlogPage;

public class FecharDriver extends BuscaBlogPage{


	public FecharDriver(WebDriver driver) {
		this.driver = driver;
	}

	//função para fechar o driver
	@After
	public void fechar_driver() {
		BuscaBlogPage.driver.quit();
	}
}
