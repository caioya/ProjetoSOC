$(document).ready(function() {var formatter = new CucumberHTML.DOMFormatter($('.cucumber-report'));formatter.uri("file:src/test/resources/features/busca_blog");
formatter.feature({
  "name": "Buscar no blog",
  "description": "\tEu como um usuário do blog do Software Integrado de Gestão Ocupacional\n\tquero buscar alguma informação na plataforma\n\tpara tirar realizar uma pesquisa",
  "keyword": "Funcionalidade"
});
formatter.scenario({
  "name": "Realizar busca no blog",
  "description": "",
  "keyword": "Cenário"
});
formatter.step({
  "name": "que estou na página do blog de saúde e segurança do trabalho",
  "keyword": "Dado "
});
formatter.match({
  "location": "BuscaBlogSteps.que_estou_na_página_do_blog_de_saúde_e_segurança_do_trabalho()"
});
formatter.result({
  "status": "passed"
});
formatter.step({
  "name": "digito a informação a ser procurada na barra de pesquisa",
  "keyword": "Quando "
});
formatter.match({
  "location": "BuscaBlogSteps.digito_a_informação_a_ser_procurada_na_barra_de_pesquisa()"
});
formatter.result({
  "status": "passed"
});
formatter.step({
  "name": "envio a solicitação da pesquisa",
  "keyword": "E "
});
formatter.match({
  "location": "BuscaBlogSteps.envio_a_solicitação_da_pesquisa()"
});
formatter.result({
  "status": "passed"
});
formatter.step({
  "name": "recebo e verifico o resultado com as informações buscadas",
  "keyword": "Então "
});
formatter.match({
  "location": "BuscaBlogSteps.recebo_e_verifico_o_resultado_com_as_informações_buscadas()"
});
formatter.result({
  "status": "passed"
});
});